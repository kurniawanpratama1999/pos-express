generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String?
  password String
  created_at DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt
  deleted_at DateTime? @default(dbgenerated("null"))
}